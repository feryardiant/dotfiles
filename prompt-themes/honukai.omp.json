{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "time",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": "[",
            "postfix": "]"
          }
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#0377C8",
          "properties": {
            "template": "{{ .UserName }}"
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#4A9207",
          "properties": {
            "prefix": "",
            "folder_icon": "\uF115",
            "folder_separator_icon": " <#ffffff>\u276F</> ",
            "style": "agnoster_short"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#B8B80A",
          "properties": {
            "prefix": "<#ffffff>on</> ",
            "template": "git:{{ .HEAD }}"
          }
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#CB4B16",
          "properties": {
            "template": "{{ .Text }}"
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#CC4B16",
          "properties": {
            "prefix": "",
            "postfix": "",
            "text": "\u276F"
          }
        }
      ]
    }
  ],
  "final_space": true
}
